{
  "name": "waitinglist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "preview": "vite preview",
    "gen": "openapi-generator-cli generate  -i ./swaggerfile.json  -g typescript-fetch -o src/ClientApi  --additional-properties=supportsES6=true,strictRequiredProps=true,operationIdNaming=camelCase",
    "gen-orval": "orval --input ./swaggerfile.json --output src/ClientApi/Api.ts",
    "gen-openapi": "npx openapi --input ./swaggerfile.json --output ./src/_api/ --client axios --useUnionTypes --additional-properties=supportsES6=true,strictRequiredProps=true",
    "gen-client": "kiota generate -l typescript -d \"./swaggerfile.json\" -o ./generated-api\n"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.11",
    "@reduxjs/toolkit": "^2.6.1",
    "aurelia-framework": "^1.4.1",
    "axios": "^1.8.4",
    "draft-js": "^0.11.7",
    "form-data": "^4.0.2",
    "openapi-typescript-codegen": "^0.29.0",
    "react": "^19.0.0",
    "react-admin": "^5.7.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.3.0",
    "vite-plugin-watch-and-run": "^1.7.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/draft-js": "^0.11.18",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "orval": "^7.9.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
